/*** default text setup ***/
body {
  background-color: <tmpl_var background_color>;
  color:            <tmpl_var text_color>;
  font-family:      Verdana,Helvetica,Arial,sans-serif;
  font-size:        12px;
  margin:			  0px 10px;
  padding:          0px;
  background-image: url("/images/left-bg.gif");
  background-repeat: repeat-y;
}

html {
	width: 770px;
	height: 100%;
}
/*** Positioning ***/

#frame {
		width:770px;
		padding:0px;
		text-align:left;
		}


#header {
  padding: 			  10px; 
  width:				  760px;
  height:           47px; 
  background-color: <tmpl_var background_color>;	
}

#header div.logo {
	position: static;
	left: 0px;
	top: 15px;
  background-color: <tmpl_var background_color>;	
}

#header div.title { 
	position: absolute;
	left: 184px;
	top: 23px; 
  width:            240px;	
  background-color: <tmpl_var background_color>;	
}

#header div.log {
  position: absolute;
	top: 23px; 
	left: 400px;
	width: 370px;
  text-align:       right;
  background-color: <tmpl_var background_color>;	 
  }

#header div.dkblu {	
  position: absolute;
  top:         40px;
  left:      180px; 
  border-top: <tmpl_var dark_color> 1px solid;
  background-color: <tmpl_var background_color>;  
  height:           1px;
  width:            590px; 
}

#header div.blue { 
  position: absolute;
  top:         43px;
  left:      10px; 
  height:          14px;  
  background-color: <tmpl_var light_color>;
  width: 760px;
}	

#left-nav {
  position:         absolute;
  left:             10px;
  top:              67px;
  width:            157px;
  background-color: <tmpl_var background_color>;
}

#content {
  width:            590px;
  margin-left:      168px;
  background-color: <tmpl_var background_color>;  
} 

#footer {
	background-color: transparent;
   color:            <tmpl_var text_color>;
	width:            760px;
	margin-top:        3px;
	clear:            both;
}

#copy {
	background-color: <tmpl_var light_color>;
   color:            <tmpl_var text_color>;
	padding-top:      4px;
	padding-bottom:   4px;
	float:            right;
	width:            590px;  
	text-align:       left;
}

#login {
	position:          absolute;
	left:              220px;
	top:               100px;
	width:             340px; 
}

/*** General Classes ***/

b {
	font-weight:      bold;
}

i {
	font-style: italic;
}

p.logl {
   float:             left;   
	padding-left:       95px;
	padding-top:        20px;
	padding-bottom:     20px;
}

p.logr {
   float:             right;   
	padding-right:     40px;
	padding-top:        20px;
	padding-bottom:     20px;
}

div.publish {
	position:          absolute;
	left:              180px;
	top:               10px;
	width:             590px; 
	text-align:        center;
}

div.box {
  border-right:     1px <tmpl_var border_color> solid;
  border-left:      1px <tmpl_var border_color> solid;
  border-bottom:    1px <tmpl_var border_color> solid;
  width:            588px;
}

div.boxb {
  border-right:     1px <tmpl_var border_color> solid;
  border-left:      1px <tmpl_var border_color> solid;
  border-bottom:    1px <tmpl_var border_color> solid;
  margin-top:       3px;
  width:            588px;
}

div.boxc {
  border:          1px <tmpl_var border_color> solid;
  margin-top:      6px;
  padding-top:     6px;
  padding-bottom:  6px;
  width:           588px;
}

div.boxc p{
	padding:       3px 3px 0px 7px;
	margin-bottom: 3px;
}

div.boxd {
  border:     1px <tmpl_var border_color> solid;
  margin-top:       6px;
  padding-top:      6px;
  width:            588px;
}

div.boxe {
  border:          1px <tmpl_var border_color> solid;
  margin-top:      6px;
  width:           588px;
}

div.boxe p{
	margin:       3px 3px 3px 3px;
}

form {
	margin: 0px;
	padding: 0px;
}

td, th {
  font-family:      Verdana,Helvetica,Arial,sans-serif;
  font-size:        12px;
}


.left2 {
  margin-top:	     0px;
  height:           auto; 
  padding-bottom:   3px;
}

/*** generic style palette ***/

.light { 
  background-color: <tmpl_var light_color>;
  color:            <tmpl_var text_color>;
  padding-top:          3px;
  padding-left:        10px;
  padding-bottom:       6px;
  margin-top: 0px;
}
.light p {
	margin-top:       0px;
	margin-bottom:    5px;
}

.bright {
  background-color: <tmpl_var bright_color>;
  color:            <tmpl_var text_color>;
}

/*** Colored UI elements ***/
.light-bar { 
  background-color: <tmpl_var light_color>;
  color:            <tmpl_var text_color>;
  padding:          4px;
  height:           12px;
}

.light-bar .left {
  width:            49%;
  float:            left;
  text-align:       left; 
}

.light-bar .right {
  width:            49%;
  float:            right;
  text-align:       right;
}

.dark-bar { 
  background-color: <tmpl_var dark_color>;
  color:            <tmpl_var light_text_color>;
  padding:          4px 4px 0px 4px;
  height:           20px;
  font-weight:      bold;
  font-family:      Verdana,Helvetica,Arial,sans-serif;
  font-size:        12px;
}

.dark-bar p.left {
	float:           left;
	margin: 0px;
}

.dark-bar p.right {
	float:           right;
	margin: 0px;
}

.dark-bar p.center {
	text-align:       center;
	margin: 0px;
}

.dark-bar2 { 
  background-color: <tmpl_var dark_color>;
  color:            <tmpl_var light_text_color>;
  padding:          4px;
  height:           20px;
  font-weight:      bold;
  font-family:      Verdana,Helvetica,Arial,sans-serif;
  font-size:        12px;
  width:            576px;
}

.button-bar {
  background-color: <tmpl_var bright_color>;
  color:            <tmpl_var text_color>;
  text-align:       right;
  padding:          3px;
  margin-top:       10px;
}

.button-bar input {
  margin-left:      15px;
}

.form-head {
  background-color: <tmpl_var light_color>;
  color:            <tmpl_var text_color>;
  border-top:       1px solid <tmpl_var border_color>;
  text-align:       left;
  padding: 			  4px;
  margin-left:      8px;
  margin-right:     8px;
  margin-top:       5px; 
  width:            571px; 
     
}

.form-head2 {
  background-color: <tmpl_var light_color>;
  color:            <tmpl_var text_color>;
  border-top:       1px solid <tmpl_var border_color>;
  text-align:       left;
  padding: 			  4px;
  margin-left:      8px;
  margin-right:     8px;
  margin-top:       5px; 
     
}
.form-cell {
  border:         1px solid <tmpl_var border_color>;
  border-collapse:collapse;
  text-align:       left;
  padding:          4px;
  margin-left:      8px;
  margin-right:     8px;   
  vertical-align:   top;
  background-color: <tmpl_var background_color>;
}

.tight-form-box {
  border:         1px solid <tmpl_var border_color>;
}

.no-border { 
  background-color: <tmpl_var background_color>;
  color:            <tmpl_var text_color>;
  padding: 3px 12px 3px 10px;
  height:           auto;
}
.no-border p.left {
	float:           left;
	margin: 0px;
}

.no-border p.right {
	float:           right;
	margin: 0px;
}

/*** prettier buttons ***/
.button {
   background-color: <tmpl_var background_color>;
   color:            <tmpl_var button_color>;
   font-family:      Verdana,Helvetica,Arial,sans-serif;
   font-size:        12px;
}

/*** form field classes ***/
.invalid { 
    background-color: <tmpl_var invalid_color>
}
.invalid textarea { 
    background-color: <tmpl_var invalid_color>
}
.invalid input    { 
    background-color: <tmpl_var invalid_color>
}
.invalid select   {
    background-color: <tmpl_var invalid_color>
}

.alert {
  color:          <tmpl_var alert_color>;
  font-weight:    bold;
  margin-bottom:  5px;
}

.alertp {
  position:      absolute;
  color:         <tmpl_var alert_color>;
  font-weight:   bold;
  width:        400px;
  left:         275px;
  top:          140px; 
}

/*** link styles ***/
a:link { 
  color:            <tmpl_var link_color>
}
a:visited { 
  color:            <tmpl_var link_color>
}
a:active {
  color:            <tmpl_var bright_color>
}

.dark-bar a { 
  color:            <tmpl_var light_text_color>;
  text-decoration:   none;
}
.dark-bar a:link { 
  color:            <tmpl_var light_text_color>;
  text-decoration:   none;
}
.dark-bar a:visited { 
  color:            <tmpl_var light_text_color>;
  text-decoration:   none;
}
.dark-bar a:active {
  color:            <tmpl_var light_text_color>;
  text-decoration:   none;
}

/*** Template Chooser Styles ***/
.tmpl_chooser_has_template { background-color: <tmpl_var light_color>; }


/*** Category Chooser Styles ***/
.hpts-outer {
     visibility:       hidden;
     position:         absolute;
     top:              0px;
     left:             0px;
     border:           2px outset <tmpl_var text_color>;
     background-color: <tmpl_var background_color>;
     filter:           progid:DXImageTransform.Microsoft.dropShadow( Color=bababa,offx=3,offy=3,positive=true);
}

.hpts-inner {
     overflow:         auto;
     width:            225px;
     height:           200px;
}

.hpts-title {
     padding:          3px;
     margin-bottom:    4px;     
     background-color: <tmpl_var dark_color>;
     color:            <tmpl_var light_text_color>;
     width:            225px;
}

.hpts-bbar {
    background-color: <tmpl_var bright_color>;
    color:            <tmpl_var text_color>;
    text-align:       right;
    padding:          3px;
    margin-top:       10px;
    width:            225px;
}

.hpts-botbar {
    background-color: <tmpl_var bright_color>;
    color:            <tmpl_var text_color>;
    width:            225px;
    font-size:        7px;
    padding:          3px;
}

.hpts-block {
     margin-left:      24px;
     display:          none;
}

.hpts-bbar .hpts-button {
    margin-left:      15px;

}

.hpts-button {
    background-color: <tmpl_var background_color>;
    color:            <tmpl_var button_color>;
    font-family:      Verdana,Helvetica,Arial,sans-serif;
}

.hpts-label-selected {
     background:       <tmpl_var light_color>;
}

.hpts-label-unselected {
     background:       <tmpl_var background_color>;
}


/*** New Left Nav Styles ***/

.dark-bar-nav { 
  background-color: <tmpl_var dark_color>;
  color:            <tmpl_var light_text_color>;
  padding:          4px 0px 0px 8px;
  height:           20px;
  font-weight:      bold;
  font-family:      Verdana,Helvetica,Arial,sans-serif;
  font-size:        12px;
}

.dark-bar-nav a:link { 
  background-color: <tmpl_var dark_color>;
  color:            <tmpl_var light_text_color>;
  font-weight:      bold;
  font-family:      Verdana,Helvetica,Arial,sans-serif;
  font-size:        12px;
  text-decoration:  none;
}
.dark-bar-nav a:visited { 
  background-color: <tmpl_var dark_color>;
  color:            <tmpl_var light_text_color>;
  font-weight:      bold;
  font-family:      Verdana,Helvetica,Arial,sans-serif;
  font-size:        12px;
}
.dark-bar-nav a:active { 
  background-color: <tmpl_var dark_color>;
  color:            <tmpl_var light_text_color>;
  font-weight:      bold;
  font-family:      Verdana,Helvetica,Arial,sans-serif;
  font-size:        12px;
}
.light-bar-nav { 
  background-color: <tmpl_var light_color>;
  color:            <tmpl_var text_color>;
  padding:          4px 4px 0px 4px;
  height:           20px;
  font-weight:      bold;
}
.light-bar-nav img {
	margin-left:    5px;
	margin-right:   5px;
}
.form-cell-nav {
  border:         1px solid <tmpl_var border_color>;
  border-top:     none;
  padding:        4px 24px;
  line-height:    150%;
  margin-bottom:  3px;
}
.plain-cell {
  padding:        2px 24px;
  line-height:    150%;
  margin-bottom:       3px;
}
.blue-border {
	background:    <tmpl_var background_color>;
   height:       3px;
}

