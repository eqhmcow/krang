<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
        "http://www.w3.org/TR/html401/loose.dtd">

<html>
  <head>
    <title><tmpl_var header_instance_name> CMS</title>
    <link rel="stylesheet" href="/krang.css" type="text/css" />
  </head>
    
    <body>
      <table border="0" cellspacing="0" cellpadding="0" width="750">
        <tr>
          <td width="155" valign="top" height="30"><img src="/images/logo.png"></td>
          <td width="15">&nbsp;</td>
          <td valign="middle" height="30" width="580">
            
            <table border="0" width="100%"><tr><td><tmpl_var header_instance_name></td><td align="right">Logged in as: <tmpl_var header_user_name> | <a href="login.pl">Logout</a></td></tr></table>
            
          </td> 
        </tr>

        <tr><td height="1" colspan="2"></td><td height="1" class="dark"></td></tr>
        <tr><td colspan="3" height="2"></td></tr>
        
        <tr><td colspan="3" class="light">&nbsp;</td></tr>
        <tr><td colspan="3" height="5"></td></tr>
      </table>


      <tmpl_loop header_message_loop>
        <div class="alert">* <tmpl_var message></div>
      </tmpl_loop>




      <tmpl_include /footer.tmpl>

    </body>
</html>
